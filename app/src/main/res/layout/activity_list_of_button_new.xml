    <Button
        android:id="@+id/lkg"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/effect_lkg"

        app:layout_constraintBottom_toTopOf="@+id/second"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/ukg"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/ukg"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="@drawable/effect_ukg"
        app:layout_constraintBottom_toTopOf="@+id/third"
        app:layout_constraintLeft_toRightOf="@id/lkg"
        app:layout_constraintRight_toLeftOf="@+id/first"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/first"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="@drawable/effect_first"
        app:layout_constraintBottom_toTopOf="@+id/forth"
        app:layout_constraintLeft_toRightOf="@id/ukg"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
