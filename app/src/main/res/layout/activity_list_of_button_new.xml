        app:layout_constraintBottom_toTopOf="@+id/seventh"
        app:layout_constraintLeft_toRightOf="@id/third"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/first" />

    <Button
        android:id="@+id/fifth"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"

        android:background="@drawable/effect_fifth"
        app:layout_constraintBottom_toTopOf="@+id/thajweedh"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/sixth"
        app:layout_constraintTop_toBottomOf="@id/second" />

    <Button
        android:id="@+id/sixth"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"

        android:background="@drawable/sixth"
        app:layout_constraintBottom_toTopOf="@+id/hifdh"
        app:layout_constraintLeft_toRightOf="@id/fifth"
        app:layout_constraintRight_toLeftOf="@+id/seventh"
        app:layout_constraintTop_toBottomOf="@id/third" />

    <Button
        android:id="@+id/seventh"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"


        android:background="@drawable/effect_seventh"
        app:layout_constraintBottom_toTopOf="@+id/batches"
        app:layout_constraintLeft_toRightOf="@id/sixth"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/forth" />

    <Button
        android:id="@+id/thajweedh"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="@drawable/effect_thajweedh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/hifdh"
        app:layout_constraintTop_toBottomOf="@id/fifth" />

    <Button
        android:id="@+id/hifdh"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="@drawable/effect_hifdh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toRightOf="@id/thajweedh"
        app:layout_constraintRight_toLeftOf="@+id/batches"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sixth" />

    <Button
        android:id="@+id/batches"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="5dp"
        android:background="@drawable/effect_batches"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toRightOf="@id/hifdh"
        app:layout_constraintRight_toRightOf="parent"

        app:layout_constraintTop_toBottomOf="@id/seventh" />
